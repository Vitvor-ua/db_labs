    "@types/morgan": "^1",
    "@types/node": "^22.15.17",
    "eslint": "^9.26.0",
    "eslint-plugin-perfectionist": "^4.12.3",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "packageManager": "npm@11.4.1"
